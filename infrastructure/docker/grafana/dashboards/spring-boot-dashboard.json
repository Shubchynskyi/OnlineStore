{
  "dashboard": {
    "title": "Spring Boot Applications",
    "tags": ["spring-boot", "java", "backend"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "JVM Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{area=\"heap\"}",
            "legendFormat": "{{service}} - {{id}}"
          }
        ]
      },
      {
        "id": 2,
        "title": "HTTP Requests Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count[5m])",
            "legendFormat": "{{service}} - {{uri}}"
          }
        ]
      },
      {
        "id": 3,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "process_cpu_usage",
            "legendFormat": "{{service}}"
          }
        ]
      },
      {
        "id": 4,
        "title": "Active Threads",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "jvm_threads_live_threads",
            "legendFormat": "{{service}}"
          }
        ]
      },
      {
        "id": 5,
        "title": "HTTP Response Times (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[5m])) by (service, uri, le))",
            "legendFormat": "{{service}} - {{uri}}"
          }
        ]
      }
    ],
    "schemaVersion": 27,
    "version": 1
  }
}
